{
  "TypeScript React Function Component": {
    "prefix": "rfc",
    "body": [
      "import React from 'react';",
      "",
      "interface ${TM_FILENAME_BASE/(.*)$/${1:/pascalcase}/}Props {",
      "\t$1",
      "}",
      "",
      "export const ${TM_FILENAME_BASE/(.*)$/${1:/pascalcase}/}: React.FC<${TM_FILENAME_BASE/(.*)$/${1:/pascalcase}/}Props> = ({ $2 }) => {",
      "\treturn (",
      "\t\t$3",
      "\t);",
      "};"
    ],
    "description": "TypeScript React Function Component"
  },
  "TypeScript Quick Export": {
    "prefix": "qex",
